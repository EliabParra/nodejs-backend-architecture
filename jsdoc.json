{
    "source": {
        "include": ["dist/src", "public/js"],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|/|\\\\)_|node_modules/|\\.tmp-starterpack/|docs/api/"
    },
    "opts": {
        "destination": "docs/api/",
        "recurse": true,
        "encoding": "utf8"
    }
}
